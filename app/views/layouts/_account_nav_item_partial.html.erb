<% notifications && notifications.class == Integer ? data_notifications = notifications : 'null' %>
<% if notifications %>
	<%= link_to user_profile_path(anchor: "todos"), class: "account-btn signed_in_btn notifcations #{(current_section == 'users') || (current_section == 'sessions') ? ' current_section' : ''}", data: {notifications: data_notifications, turbolinks: false} do %>
		<%= icomoon('user') %>
	<% end %>
<% else %>
	<%= link_to user_profile_path, class: "account-btn signed_in_btn#{(current_section == 'users') || (current_section == 'sessions') ? ' current_section' : ''}", data: {notifications: data_notifications} do %>
		<%= icomoon('user') %>
	<% end %>
<% end %>